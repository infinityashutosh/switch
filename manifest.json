{
    "manifest_version": 3,
    "name": "Browser Window Live Stream",
    "version": "1.0.0",
    "description": "Live stream your current browser window",
    "permissions": [
      "desktopCapture",
      "activeTab"
    ],
    "host_permissions":["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons":{
      "128":"logo.png"
    }
  }